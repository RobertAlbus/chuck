public class MidiIntervals {

  // base
  1 =>  float semi;
  2 =>  float whole;
  12 => float oct;

  // perfect
  0  => float P1;
  5  => float P4;
  7  => float P5;
  12 => float P8;

  // major
  2  => float M2;
  4  => float M3;
  9  => float M6;
  11 => float M7;

  // minor
  1  => float m2;
  3  => float m3;
  8  => float m6;
  10 => float m7;

  // tritone
  6  => float TT;
  TT => float a4;
  TT => float d5;

}